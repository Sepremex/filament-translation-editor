{
    "name": "sepremex/filament-translation-editor",
    "description": "Manage language files with ease.",
    "keywords": ["laravel", "localization", "multilingual", "Filament"],
    "homepage": "https://github.com/sepremex/filament-translation-editor",
    "license": "MIT",
    "authors": [
        {
            "name": "SEPREMEX",
            "email": "servicio@sepremex.com",
            "homepage": "https://sepremex.com/"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Sepremex\\FilamentTranslationEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sepremex\\FilamentTranslationEditor\\FilamentTranslationEditorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.0.6"
}
